<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yang.sysmanagementnew.mapper.LeaveMapper">

    <update id="UpdLeaveInfo" parameterType="Map" >
        update leave_info set leave_no=#{leaveNo},leave_state=#{leaveState} where leave_id = #{leaveId}
    </update>
</mapper>
